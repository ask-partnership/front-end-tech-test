<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ASK Technical Test</title>
</head>
<body>
    <header>
        <h1>@ViewBag.Title</h1>
    </header>
    <section>
        @RenderBody()
    </section>
    <footer>
        <p>&copy; @DateTime.Now.Year - ASK</p>
    </footer>

    @RenderSection("scripts", required: false)
</body>
</html>
